                                        <i class="far fa-trash-alt tm-product-delete-icon"></i>
                                    </a>
                                </td>
                            </tr>
                            {{/data.methodPayment}}
                        </tbody>
                    </table>
                </div>
                <!-- table container -->
                <a href="/admin/addmethodpayment" class="btn btn-primary btn-block text-uppercase mb-3">Add new</a>
            </div>
        </div>
        <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12 tm-block-col">
            <div class="tm-bg-primary-dark tm-block tm-block-products">
                <div class="tm-product-table-container">
                    <table class="table table-hover tm-table-small tm-product-table">
                        <thead>
                            <tr>
                                <th scope="col">CITY</th>
                                <th scope="col">ZIPCODE</th>
                                <th scope="col">SHIPPER PRICE</th>
                                <th scope="col">SLUG</th>
                                <th scope="col">&nbsp;</th>
                            </tr>
                        </thead>
                        <tbody>
                            {{#data.cities}}
                            <tr id="city-{{id}}">
                                <td class="tm-product-name" onclick="viewCity({{id}})" style="cursor: pointer;">
                                    {{name}}
                                </td>
                                <td>{{zipcode}}</td>
                                <td>{{price}}</td>
                                <td>{{slug}}</td>
                                <td>                                    
                                    <a onclick="deleteCity({{id}})" class="tm-product-delete-link">
                                        <i class="far fa-trash-alt tm-product-delete-icon"></i>
                                    </a>
                                </td>
                            </tr>
                            {{/data.cities}}
                        </tbody>
                    </table>
                </div>
                <!-- table container -->
                <a href="/admin/addcity" class="btn btn-primary btn-block text-uppercase mb-3">Add new</a>
            </div>
        </div>
        <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12 tm-block-col">
