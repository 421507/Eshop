<div class="container mt-5">
    <div class="row tm-content-row">
        <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12 tm-block-col">
            <div class="tm-bg-primary-dark tm-block tm-block-products">
                <div class="tm-product-table-container">
                    <table class="table table-hover tm-table-small tm-product-table">
                        <thead>
                            <tr>
                                <th scope="col">STATUS DELIVERY</th>
                                <th scope="col">SLUG</th>
                                <th scope="col">&nbsp;</th>
                            </tr>
                        </thead>
                        <tbody>
                            {{#data.statusDelivery}}
                            <tr id="statusdelivery-{{id}}">
                                <td class="tm-product-name" onclick="viewStatusDelivery({{id}})" style="cursor: pointer;">
                                    {{name}}
                                </td>
                                <td>{{slug}}</td>
                                <td>                                    
                                    <a onclick="deleteStatusDelivery({{id}})" class="tm-product-delete-link">
                                        <i class="far fa-trash-alt tm-product-delete-icon"></i>
                                    </a>
                                </td>
                            </tr>
                            {{/data.statusDelivery}}
                        </tbody>
                    </table>
                </div>
                <!-- table container -->
                <a href="/admin/addstatusdelivery" class="btn btn-primary btn-block text-uppercase mb-3">Add new</a>
            </div>
        </div>
        <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12 tm-block-col">
            <div class="tm-bg-primary-dark tm-block tm-block-products">
                <div class="tm-product-table-container">
                    <table class="table table-hover tm-table-small tm-product-table">
                        <thead>
                            <tr>
                                <th scope="col">METHOD PAYMENT</th>
